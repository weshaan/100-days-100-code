// character identifier

#include<stdio.h>
#include<string.h>

int character(char ch);
int main()
{
	printf("Please enter your word...\n");
	char word[100];
	gets(word);
    int a;
    int siz=sizeof(word);
    
    for(a=0;;a++)
    {
    	
		if((a!=0)&&(a!=1)&&(a!=2)&&(a!=3)&&(a!=4)&&(a!=5)&&(a!=6)&&(a!=7)&&(a!=8)&&(a!=9))
		if(a%8==0)
    	{
    		if(word[a]==9);
    		{a++;
			continue;
		}}
    	character (word[a]);
    	
    	if(a==siz)
    	{break;
		}
	}
}

