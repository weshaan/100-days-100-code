#include <stdio.h>
#include <stdlib.h>
#include<time.h>

int main(void) {
  srand(time(0));
  
    time_t seconds; 
      
    seconds = time(NULL); 
    printf("Seconds since January 1, 1970 = %ld \n", seconds); 

  printf("5 random number:...\n");
  for (int i = 0; i < 10; i++)
    printf(" %d ", rand());

  return 0;
}
